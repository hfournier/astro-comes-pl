import assets_images_wizualizacje_placu_zabaw_wizualizacja_jpg_2_jpg from "@assets/images/wizualizacje-placu-zabaw/wizualizacja-jpg-2.jpg";
import assets_images_wizualizacje_placu_zabaw_wizualizacja_jpg_1_jpg from "@assets/images/wizualizacje-placu-zabaw/wizualizacja-jpg-1.jpg";

const wpId = 3176
const title = "Wizualizacje placu zabaw"
const description = "TODO: Add description"
